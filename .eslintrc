{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"es6": true,
		"jquery": true,
		"node": true
	},
	"globals": {
		"angular": true,
		"moment": true,
		"_": true
	},
	"rules": {
		"eol-last": ["error", "always"],
		"indent": ["error", "tab"],
		"quotes": ["error", "single"],
		"no-mixed-requires": ["off"],
		"no-underscore-dangle": ["off"]
	},
	"parserOptions": {
		"sourceType": "module"
	}
}
